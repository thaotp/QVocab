 <div class="un-panel">
  <div class="panel-body container-fluid">
    <div class="row row-lg">

      <div class="col-sm-6">
      
        <div class="login-wrap">
           
          <div class="login">
            <%= form_for(resource, as: resource_name, url: session_path(resource_name), class: 'form-login') do |f| %>
              <div class="form-group">
                <%= f.email_field :email, autofocus: true, class: 'form-control round', placeholder:  'Email Address', autocomplete: 'off' %>
              </div>
              <div class="form-group">
                <%= f.password_field :password, autocomplete: "off", class: 'form-control round', placeholder: 'Password', autocomplete: 'off' %>
              </div>

              <div class="form-group">
                <%= f.submit("Log in", class: 'btn btn-round btn-outline btn-primary btn-sm') %>
              </div>
            <% end %>
          </div>
        </div>

      </div>

    <div class="clearfix hidden-xs"></div>
  </div>
</div>
